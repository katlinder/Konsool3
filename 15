using System;

namespace ConsoleApp11
{
    class Program
    {
        static void Main()
        {
            int[] Numbers = new int[3];
            //valik
            Numbers[0] = 101;
            Numbers[1] = 102;
            Numbers[2] = 103;

          
            
            foreach(int k in Numbers)
            {
                Console.WriteLine(k);//võtab väärtused ja prindib kui antud k väärtusena
            }
            
            for(int j=0; j< Numbers.Length; j++) //määrab väärtuse, võrdleb seda ja siis lisab juurde
            {
                Console.WriteLine(Numbers[j]);
            }

            int i = 0; //i on 0
            while (i < Numbers.Length) //i (0) on väiksem kui 3
            {
            Console.WriteLine(Numbers[i]); //võtab i väärtuse valikust
                i++; //liidab i'le +1 ja võrdleb uuesti 3ga
            }
        }
    }
}
